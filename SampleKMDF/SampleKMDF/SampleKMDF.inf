;
; SampleKMDF.inf
;

[Version]
Signature="$WINDOWS NT$"
Class=Sample ; TODO: edit Class
ClassGuid={78A1C341-4539-11d3-B88D-00C04FAD5171} ; TODO: edit ClassGuid
Provider=%ManufacturerName%
CatalogFile=SampleKMDF.cat
DriverVer= ; TODO: set DriverVer in stampinf property pages

[DestinationDirs]
DefaultDestDir = 12
SampleKMDF_Device_CoInstaller_CopyFiles = 11

; ================= Class section =====================

[ClassInstall32]
Addreg=SampleClassReg

[SampleClassReg]
HKR,,,0,%ClassName%
HKR,,Icon,,-5

[SourceDisksNames]
1 = %DiskName%,,,""

[SourceDisksFiles]
SampleKMDF.sys  = 1,,
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll=1 ; make sure the number matches with SourceDisksNames

;*****************************************
; Install Section
;*****************************************

[Manufacturer]
%ManufacturerName%=Standard,NT$ARCH$

[Standard.NT$ARCH$]
%SampleKMDF.DeviceDesc%=SampleKMDF_Device, Root\SampleKMDF ; TODO: edit hw-id

[SampleKMDF_Device.NT]
CopyFiles=Drivers_Dir

[Drivers_Dir]
SampleKMDF.sys

;-------------- Service installation
[SampleKMDF_Device.NT.Services]
AddService = SampleKMDF,%SPSVCINST_ASSOCSERVICE%, SampleKMDF_Service_Inst

; -------------- SampleKMDF driver install sections
[SampleKMDF_Service_Inst]
DisplayName    = %SampleKMDF.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\SampleKMDF.sys

;
;--- SampleKMDF_Device Coinstaller installation ------
;

[SampleKMDF_Device.NT.CoInstallers]
AddReg=SampleKMDF_Device_CoInstaller_AddReg
CopyFiles=SampleKMDF_Device_CoInstaller_CopyFiles

[SampleKMDF_Device_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll,WdfCoInstaller"

[SampleKMDF_Device_CoInstaller_CopyFiles]
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll

[SampleKMDF_Device.NT.Wdf]
KmdfService =  SampleKMDF, SampleKMDF_wdfsect
[SampleKMDF_wdfsect]
KmdfLibraryVersion = $KMDFVERSION$

[Strings]
SPSVCINST_ASSOCSERVICE= 0x00000002
ManufacturerName="<Your manufacturer name>" ;TODO: Replace with your manufacturer name
ClassName="Samples" ; TODO: edit ClassName
DiskName = "SampleKMDF Installation Disk"
SampleKMDF.DeviceDesc = "SampleKMDF Device"
SampleKMDF.SVCDESC = "SampleKMDF Service"
